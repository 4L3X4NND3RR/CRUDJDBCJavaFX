<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ProgressIndicator?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="pane" prefHeight="167.0" prefWidth="305.0" styleClass="grid-pane" stylesheets="@../styles/styles.css"
            xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.alexander.viewcontrollers.CategoryRegisterController">
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
   </padding>
   <TextField fx:id="txtId" editable="false" layoutX="20.0" layoutY="36.0" prefHeight="29.0" prefWidth="75.0"
              promptText="Id"/>
   <TextField fx:id="txtNameCategory" layoutX="20.0" layoutY="89.0" prefHeight="29.0" prefWidth="265.0"
              promptText="Nombre categoria"/>
   <Button layoutX="20.0" layoutY="159.0" mnemonicParsing="false" onAction="#handlerBtnSave" prefHeight="27.0"
           prefWidth="100.0" text="Guardar"/>
   <Button layoutX="185.0" layoutY="159.0" mnemonicParsing="false" onAction="#handlerBtnSalir" prefHeight="27.0"
           prefWidth="100.0" text="Salir"/>
   <ProgressIndicator fx:id="pIndicator" layoutX="110.0" layoutY="24.0" visible="false"/>
</AnchorPane>
